html

head
    title Program
    meta(charset='UTF-8')

body
    h1 View user details
    a(href='/') Home

    p Name: #{user.name}
    
    p Username: #{user.username}
   
    p Email: #{user.email}
    
    p Street: #{user.street}
 
    p Suite: #{user.suite}
    
    p City: #{user.city}
    
    p Zipcode: #{user.zipcode}
    
    p Latitude: #{user.lat}
   
    p Longitude: #{user.lng}
    
    p Phone: #{user.phone}
    
    p Website: #{user.website}
    
    p Company Name: #{user.companyName}
   
    p Company Catchphrase: #{user.companyCatchphrase}
    
    p Company BS: #{user.companyBs}